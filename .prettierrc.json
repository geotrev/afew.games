{
  "semi": false,
  "printWidth": 80,
  "tabWidth": 2,
  "arrowParens": "always",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "printWidth": 120
      }
    }
  ],
  "plugins": ["prettier-plugin-tailwindcss"],
  "pluginSearchDirs": false
}
