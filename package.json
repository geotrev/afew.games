    "next": "^13.1.5",
    "path": "^0.12.7",
    "prettier": "^2.8.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1",
    "xss": "^1.0.14"
