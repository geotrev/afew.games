    "marked": "^4.0.17",
    "next": "^12.2.0",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sass": "^1.53.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run prettier:check",
      "npm run eslint:check"
    ],
    "*.(json|scss)": [
      "npm run prettier:write"
    ]
  },
  "commithelper": {
    "types": [
